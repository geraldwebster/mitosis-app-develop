{
  "name": "@jsx-lite/core",
  "version": "0.0.2",
  "homepage": "https://github.com/BuilderIO/jsx-lite",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/BuilderIO/jsx-lite"
  },
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "release:dev": "npm run build && npm version prerelease --no-git-tag-version && ALLOW_PUBLISH=true npm publish",
    "release:patch": "npm run build && npm version patch --no-git-tag-version && ALLOW_PUBLISH=true npm publish",
    "start": "npm run build -- --watch",
    "ts-node": "ts-node -O '{\"module\": \"commonjs\"}'",
    "build:declaration": "tsc --project tsconfig.build.json",
    "build:es": "BABEL_ENV=build babel src --root-mode upward --out-dir dist --source-maps --extensions .ts,.tsx --delete-dir-on-start --no-comments"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-syntax-jsx": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "axios-cache-adapter": "^2.5.0",
    "chalk": "^4.1.0",
    "csstype": "^3.0.4",
    "axios": "^0.21.0",
    "dedent": "^0.7.0",
    "liquidjs": "^9.6.0",
    "json5": "^2.1.3",
    "prettier": "^2.1.2",
    "traverse": "^0.6.6",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5",
    "vue-template-compiler": "^2.6.12",
    "lodash": "^4.17.20",
    "object-hash": "^2.0.3"
  },
  "devDependencies": {
    "@builder.io/sdk": "^1.1.20",
    "@jsx-lite/eslint-plugin": "../eslint-plugin",
    "@testing-library/jest-dom": "^5.11.5",
    "@types/babel__core": "^7.1.12",
    "@types/dedent": "^0.7.0",
    "@types/jest": "^26.0.15",
    "@types/json5": "0.0.30",
    "@types/lodash": "^4.14.165",
    "@types/object-hash": "^1.3.4",
    "@types/prettier": "^2.1.5",
    "@types/traverse": "^0.6.32",
    "jest": "^26.6.3",
    "jest-raw-loader": "^1.0.1",
    "ts-jest": "^26.4.4"
  }
}
